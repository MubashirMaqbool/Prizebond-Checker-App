<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ButtonStyle" parent="Base.Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/colorAccent</item>
    </style>

    <style name="MyEditTextStyle2" parent="Widget.AppCompat.EditText">
        <item name="colorControlActivated">#060606</item>
        <item name="colorControlNormal">#060606</item>
    </style>


    <style name="create_account" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/color4</item>
        <item name="colorPrimaryDark">@color/color3</item>
        <item name="colorAccent">@color/color4</item>
    </style>

    <style name="dash_board" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/color4</item>
        <item name="colorPrimaryDark">@color/color3</item>
        <item name="colorAccent">@color/color4</item>
    </style>

    <style name="login_page" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/color4</item>
        <item name="colorPrimaryDark">@color/color3</item>
        <item name="colorAccent">@color/color4</item>
    </style>

    <style name="first_page" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/color1</item>
        <item name="colorPrimaryDark">@color/color1</item>
        <item name="colorAccent">@color/color1</item>
    </style>

    <style name="how_to" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/color4</item>
        <item name="colorPrimaryDark">@color/color3</item>
        <item name="colorAccent">@color/color4</item>
    </style>

    <style name="single" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/color4</item>
        <item name="colorPrimaryDark">@color/color3</item>
        <item name="colorAccent">@color/color4</item>
    </style>

    <style name="range" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/color4</item>
        <item name="colorPrimaryDark">@color/color3</item>
        <item name="colorAccent">@color/color4</item>
    </style>

    <style name="ThemeSettings" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/violet</item>
        <item name="colorPrimaryDark">@color/violet_dark</item>
        <item name="colorAccent">@color/violet</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
